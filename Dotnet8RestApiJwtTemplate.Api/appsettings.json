{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Default": "Server=Host;Database=DB;Trusted_Connection=True;"
  },
  "IsShowSwagger": true,
  "SwaggerUrl": "/",
  "UrlService": {
  },
  "Jwt": {
    "Key": "replace-with-64+chars-random-secret",
    "Issuer": "Dotnet8RestApiJwtTemplate",
    "Audience": "Dotnet8RestApiJwtTemplate.Client",
    "AccessTokenMinutes": 60
  }
}
